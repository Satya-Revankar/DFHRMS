<?xml version="1.0" encoding="utf-8"?>
 <!--   College name - Not required
    Class mode - required
    Semester - Not Required
    Subject name - Required
    Topic name - Required
    Attendance main id - Not Required
    Class start time - Required
    Class end time - Required
    Attendance start time - Required
    Attendance end time - Required
    Total - Required
    Present - Required
    Absent - Required
    Image uploaded - Not Required
    Head count - Not Required
    Additional - Trainer name, Rating-->
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginLeft="20dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Trainer Name : "
            android:textColor="@color/greenTextcolor">
        </TextView>
        <TextView
            android:id="@+id/trainername_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Default name">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Class Mode : "
            android:textColor="@color/greenTextcolor">
        </TextView>

        <TextView
            android:id="@+id/classMode_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Default mode">
        </TextView>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Subject Name : "
            android:textColor="@color/greenTextcolor">
        </TextView>

        <TextView
            android:id="@+id/subject_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Default subject"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Topic Name : "
            android:textColor="@color/greenTextcolor">
        </TextView>

        <TextView
            android:id="@+id/topic_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="Default Topic"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/classstart_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Class Start Time : "
            android:textColor="@color/greenTextcolor">
        </TextView>
        <TextView
            android:id="@+id/classStartTime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="00:00:00 PM">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/classend_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Class End Time : "
            android:textColor="@color/greenTextcolor">

        </TextView>
        <TextView
            android:id="@+id/classEndTime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="00:00:00 PM">
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/attendanceStart_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Attendance Start Time : "
            android:textColor="@color/greenTextcolor">
        </TextView>
        <TextView
            android:id="@+id/attendanceStartTime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="00:00:00 PM">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/attendanceEnd_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Attendance End Time : "
            android:textColor="@color/greenTextcolor">

        </TextView>
        <TextView
            android:id="@+id/attendanceEndTime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="00:00:00 PM">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/total_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Total Students : "
            android:textColor="@color/greenTextcolor">
        </TextView>
        <TextView
            android:id="@+id/total_students_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="0">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/present_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Present Students : "
            android:textColor="@color/greenTextcolor">
        </TextView>
        <TextView
            android:id="@+id/present_students_tv"
            android:layout_width="wrap_content"
            android:textColor="@color/black"
            android:layout_height="wrap_content"
            android:text="0">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/absent_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Absent Students : "
            android:textColor="@color/greenTextcolor">
        </TextView>
        <TextView
            android:id="@+id/absent_students_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="0">
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ratings : "
            android:textColor="@color/greenTextcolor">
        </TextView>
        <TextView
            android:id="@+id/ratings_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="0">
        </TextView>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#23000000"
        android:layout_marginTop="10dp"/>
</LinearLayout>